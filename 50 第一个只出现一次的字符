class Solution {
public:
    char firstUniqChar(string s) {
        // 有序哈希表 按照插入顺序排序
        unordered_map<char, int> cnt;
        vector<char> unique;
        for (char c : s) {
            if (cnt[c] == 0) {
                unique.emplace_back(c);
            }
            cnt[c]++;
        }
        for (char c : unique) {
            if (cnt[c] == 1) {
                return c;
            }
        }
        return ' ';
    }
};
