# Definition for singly-linked list.
# class ListNode:
#     def __init__(self, val=0, next=None):
#         self.val = val
#         self.next = next
class Solution:
    def reverseBetween(self, head: Optional[ListNode], left: int, right: int) -> Optional[ListNode]:
        dummy = ListNode(next=head)
        prev = dummy
        for i in range(left - 1):
            prev = prev.next
        
        last_unchange, cur = prev, prev.next
        
        for i in range(right - left + 1):
            third = cur.next
            cur.next = prev
            prev, cur = cur, third
            
        last_unchange.next.next = cur
        last_unchange.next = prev
        return dummy.next
