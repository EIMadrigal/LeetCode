#
# 代码中的类名、方法名、参数名已经指定，请勿修改，直接返回方法规定的值即可
#
# 
# @param n int整型 
# @return int整型
#
class Solution:
    def findNthDigit(self, n: int) -> int:
        l, r = 0, 10
        digits_num = 1
        while n > (r - l) * digits_num:
            n -= (r - l) * digits_num
            digits_num += 1
            l, r = r, r * 10
        q, r = divmod(n, digits_num)
        return int(str(l + q)[r])
