摩尔投票法
class Solution {
public:
    int majorityElement(vector<int>& nums) {
        int ans = nums[0], cnt = 0;
        for (int num : nums) {
            if (ans == num) {
                ++cnt;
            }
            else {
                if (cnt == 1) {
                    ans = num;
                }
                else {
                    cnt--;
                }
            }
        }
        return ans;
    }
};
