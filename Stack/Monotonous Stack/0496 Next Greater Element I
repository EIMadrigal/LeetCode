class Solution:
    def nextGreaterElement(self, nums1: List[int], nums2: List[int]) -> List[int]:
        stack = []
        ans = {}
        for num in nums2:
            while stack and num > stack[-1]:
                cur = stack.pop()
                ans[cur] = num
            stack.append(num)
        while stack:
            cur = stack.pop()
            ans[cur] = -1
        return [ans[num] for num in nums1]
