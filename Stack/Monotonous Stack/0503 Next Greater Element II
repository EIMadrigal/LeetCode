# 对循环数组，取余访问2倍数组即可

class Solution:
    def nextGreaterElements(self, nums: List[int]) -> List[int]:
        ans = [-1] * len(nums)
        stack = []
        for i in range(len(nums) * 2 - 1):
            while stack and nums[i % len(nums)] > nums[stack[-1]]:
                cur = stack.pop()
                ans[cur] = nums[i % len(nums)]
            stack.append(i % len(nums))
        return ans
