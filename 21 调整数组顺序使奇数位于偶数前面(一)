#
# 代码中的类名、方法名、参数名已经指定，请勿修改，直接返回方法规定的值即可
#
# 
# @param array int整型一维数组 
# @return int整型一维数组
#
class Solution:
    def reOrderArray(self, array: List[int]) -> List[int]:
        j = 0  # 奇数插入位置
        for i, num in enumerate(array):
            if num % 2:
                for k in range(i - 1, j - 1, -1):
                    array[k + 1] = array[k]
                array[j] = num
                j += 1
        return array
