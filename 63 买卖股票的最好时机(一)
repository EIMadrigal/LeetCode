#
# 代码中的类名、方法名、参数名已经指定，请勿修改，直接返回方法规定的值即可
#
# 
# @param prices int整型一维数组 
# @return int整型
#
class Solution:
    def maxProfit(self, prices: List[int]) -> int:
        # 只要维护历史最低价格即可
        min_price = prices[0]
        ans = 0
        for price in prices:
            min_price = min(min_price, price)
            ans = max(ans, price - min_price)
        return ans
