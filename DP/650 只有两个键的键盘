class Solution {
public:
    int minSteps(int n) {
        vector<int> dp(n + 1, INT_MAX);
        dp[0] = 0, dp[1] = 0;
        for (int i = 2; i <= n; ++i) {
            for (int j = 1; j < i; ++j) {
                // 1次copy + i / j - 1次paste
                if (i % j == 0) {
                   dp[i] = min(dp[i], dp[j] + i / j);
                }
            }
        }
        return dp[n];
    }
};
